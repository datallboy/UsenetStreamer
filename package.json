{
  "name": "usenetstreamer",
  "version": "1.7.3",
  "description": "Usenet-powered web streaming bridge for Stremio",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "build:parser": "tsc -p tsconfig.parser.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc -b tsconfig.shared.json tsconfig.integrations.json tsconfig.services.json tsconfig.app.json && tsc --noEmit -p tsconfig.json",
    "typecheck": "npm run type-check",
    "fixtures:capture-core": "node scripts/capture-golden-core.js",
    "fixtures:check-core": "node scripts/check-golden-core.js",
    "smoke:endpoints": "node scripts/smoke-endpoints.js",
    "check:layer-js": "node scripts/check-layer-js-allowlist.js",
    "check:layers": "node scripts/check-layer-boundaries.js",
    "test:stream-same-output": "node --test tests/stream-same-output/getStreamsUseCase.same-output.test.js",
    "docs:jsdoc:clean": "node -e \"require('node:fs').rmSync('docs/jsdoc', { recursive: true, force: true })\"",
    "docs:jsdoc": "npm run docs:jsdoc:clean && jsdoc -c jsdoc.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "form-data": "^4.0.1",
    "lzma-native": "^8.0.6",
    "morgan": "^1.10.0",
    "nntp": "^0.3.1",
    "streamsearch": "^0.1.2",
    "stremio-addon-sdk": "^1.6.10",
    "user-agents": "^1.1.314",
    "webdav": "^5.6.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/node": "^25.3.1",
    "jsdoc": "^4.0.5",
    "typescript": "^5.9.3"
  }
}
